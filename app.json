{
  "expo": {
    "name": "canine-cupid-mobile",
    "slug": "canine-cupid-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryAddUsageDescription": "This app uses the camera roll of the user's iphone to select photos from camera roll",
        "permissions": [
          "CAMERA",
          "ACCESS_FINE_LOCATION",
          "USE_FINGERPRINT"
        ]
      },
      "bundleIdentifier": "com.lrmccann.canine-cupid-mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "just publishing so i can get my bundle id for firebase"
  }
}